<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FaceDetectView">
        <attr name="autoSwitchToNewDevice" format="boolean" />
        <attr name="showTrackRect" format="boolean" />
        <attr name="rectColor" format="color" />
        <attr name="rectWidth" format="dimension" />
        <attr name="enableSample" format="boolean" />
        <attr name="sampleInterval" format="integer" />
        <attr name="previewResolution" format="enum">
            <enum name="LOWEST" value="0" />
            <enum name="HIGHEST" value="1" />
        </attr>
        <attr name="taskRunner" format="enum">
            <enum name="COROUTINE" value="0" />
            <enum name="RX" value="1" />
        </attr>
    </declare-styleable>
</resources>